{
  "name": "@chatwoot/mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:publish-internal": "cd android && ./gradlew clean && fastlane android beta_internal",
    "android:publish-open": "cd android && ./gradlew clean && fastlane android beta_open",
    "android:release": "react-native run-android --variant=release",
    "build-storybook": "build-storybook",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean --force",
    "clean:hard": "react-native-clean-project",
    "ios": "react-native run-ios",
    "ios:clean": "./react-native-setup-ios.sh clean",
    "ios:install": "./react-native-setup-ios.sh install",
    "ios:publish-internal": "cd ios && fastlane ios beta",
    "ios:setup": "./react-native-setup-ios.sh setup",
    "lint": "eslint .",
    "pod-install": "cd ios && bundle exec pod install",
    "prepare": "husky install",
    "start": "yarn clean && react-native start",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "test": "jest"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.7",
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/devtools": "^6.0.19",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "i18n-js": "^3.8.0",
    "lodash.filter": "^4.6.0",
    "lodash.groupby": "^4.6.0",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-native": "0.72.4",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^5.6.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-reanimated": "^3.4.2",
    "react-native-safari-view": "^2.1.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-snackbar": "^2.6.2",
    "react-native-svg": "^13.12.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.212.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2"
  },
  "engines": {
    "node": ">=16"
  }
}
